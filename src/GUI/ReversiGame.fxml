<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.text.Text?>
<HBox fx:id="root" spacing="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ReversiGameController">
    <VBox prefHeight="397.0" prefWidth="176.0" spacing="15.0">
        <Label alignment="BOTTOM_CENTER" contentDisplay="CENTER" prefHeight="16.0" prefWidth="177.0" text="Reversi" textAlignment="CENTER" underline="true" />
      <Text fx:id="playerTurn" text="Label" />
      <Text fx:id="playerOnePoints" text="Label" />
      <Text fx:id="playerTwoPoints" text="Label" />
      <Pane prefHeight="294.0" prefWidth="176.0" />
        <Button fx:id="menuButton" onAction="#callMenuBack" prefHeight="0.0" prefWidth="176.0" text="Menu">
         <VBox.margin>
            <Insets />
         </VBox.margin></Button>
        <Button fx:id="quitButton" alignment="BOTTOM_CENTER" onAction="#quit" prefHeight="0.0" prefWidth="176.0" text="Quit">
         <VBox.margin>
            <Insets />
         </VBox.margin></Button>
      <HBox.margin>
         <Insets bottom="20.0" top="20.0" />
      </HBox.margin>
    </VBox>
</HBox>
